@prefix : <http://example.org/legal-ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/legal-ontology> a owl:Ontology ;
    rdfs:label "Complete Legal Ontology" ;
    rdfs:comment "Comprehensive ontology of law built on BFO, the Social Ontology of Law, the Minimum Legal Chain (MLC), structural violation classes, tort ontology, and offense mapping." .

:Continuant a owl:Class ;
    rdfs:label "Continuant" ;
    rdfs:comment "An entity that persists through time while maintaining identity." .

:Occurrent a owl:Class ;
    rdfs:label "Occurrent" ;
    rdfs:comment "An entity that unfolds through time, such as a process or event." .

:Role a owl:Class ;
    rdfs:label "Role" ;
    rdfs:comment "A realizable dependent continuant that inheres in a bearer and is realized in processes." .

:Disposition a owl:Class ;
    rdfs:label "Disposition" ;
    rdfs:comment "A quality that can produce certain behaviors when triggered." .

:RealizableDependent a owl:Class ;
    rdfs:label "Realizable dependent" ;
    rdfs:comment "A dependent continuant that can be realized (e.g., rights, obligations)." .

:RelationalDependent a owl:Class ;
    rdfs:label "Relational dependent" ;
    rdfs:comment "A dependent continuant that depends on a relation among bearers." .

:System a owl:Class ;
    rdfs:label "System" ;
    rdfs:comment "A complex whole composed of interrelated parts." .

:Process a owl:Class ;
    rdfs:label "Process" ;
    rdfs:comment "A kind of occurrent (event or activity)." .

:LegalSystem a owl:Class ;
    rdfs:label "Legal system" ;
    rdfs:comment "A historically and socially situated system of norms and practices." .

:Norm a owl:Class ;
    rdfs:label "Norm" ;
    rdfs:comment "A rule or standard within a legal system." .

:Agent a owl:Class ;
    rdfs:label "Agent" ;
    rdfs:comment "An entity that can bear roles and perform acts (person, organization, state actor)." .

:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "An individual human agent." .

:Organization a owl:Class ;
    rdfs:label "Organization" ;
    rdfs:comment "A collective agent such as a corporation, institution, or government body." .

:LegalEvent a owl:Class ;
    rdfs:label "Legal event" ;
    rdfs:comment "A temporally extended occurrence involving agents, norms, roles, procedures, and facts." .

:NormativeEvent a owl:Class ;
    rdfs:label "Normative event" ;
    rdfs:comment "A legal event concerning creation, modification, or application of norms." .

:OffenseEvent a owl:Class ;
    rdfs:label "Offense event" ;
    rdfs:comment "A legal event that instantiates a wrongful pattern described by an offense type." .

:FactSituation a owl:Class ;
    rdfs:label "Fact situation" ;
    rdfs:comment "A concrete factual configuration relevant to legal assessment." .

:Remedy a owl:Class ;
    rdfs:label "Remedy" ;
    rdfs:comment "A structured legal response intended to repair, compensate, punish, or restore." .

:Institution a owl:Class ;
    rdfs:label "Institution" ;
    rdfs:comment "A structured organization with legal authority and functions." .

:Executive a owl:Class ;
    rdfs:label "Executive" ;
    rdfs:comment "An institution responsible for enforcing laws." .

:Registry a owl:Class ;
    rdfs:label "Registry" ;
    rdfs:comment "An institution responsible for recording and maintaining legal facts." .

:Legislature a owl:Class ;
    rdfs:label "Legislature" ;
    rdfs:comment "An institution with the authority to create norms." .

:Court a owl:Class ;
    rdfs:label "Court" ;
    rdfs:comment "An institution responsible for adjudication." .

:LegalFact a owl:Class ;
    rdfs:label "Legal fact" ;
    rdfs:comment "A fact recognized by law and which has legal significance." .

:Liability a owl:Class ;
    rdfs:label "Liability" ;
    rdfs:comment "A legal fact indicating responsibility for wrongdoing." .

:Ownership a owl:Class ;
    rdfs:label "Ownership" ;
    rdfs:comment "A legal fact indicating possession and control over property." .

:Validity a owl:Class ;
    rdfs:label "Validity" ;
    rdfs:comment "A legal fact indicating that a norm or act meets formal requirements." .

:Standing a owl:Class ;
    rdfs:label "Standing" ;
    rdfs:comment "A legal fact indicating the capacity of a party to bring a claim." .

:NormativeStatus a owl:Class ;
    rdfs:label "Normative status" ;
    rdfs:comment "A status that qualifies the legal capacity or standing of an agent." ;
    rdfs:subClassOf :Norm .

:Right a owl:Class ;
    rdfs:label "Right" ;
    rdfs:comment "A normative status conferring entitlements." ;
    rdfs:subClassOf :Norm .

:Obligation a owl:Class ;
    rdfs:label "Obligation" ;
    rdfs:comment "A normative status requiring a bearer to act or refrain from acting." ;
    rdfs:subClassOf :Norm .

:Prohibition a owl:Class ;
    rdfs:label "Prohibition" ;
    rdfs:comment "A normative status forbidding an action." ;
    rdfs:subClassOf :Norm .

:Permission a owl:Class ;
    rdfs:label "Permission" ;
    rdfs:comment "A normative status allowing an action." ;
    rdfs:subClassOf :Norm .

:NormativeRole a owl:Class ;
    rdfs:label "Normative role" ;
    rdfs:comment "A normative continuant conferring legal capacity within an institution." ;
    rdfs:subClassOf :Norm .

:NormativePower a owl:Class ;
    rdfs:label "Normative power" ;
    rdfs:comment "Capacity to alter rights, obligations, or other normative statuses." ;
    rdfs:subClassOf :Norm .

:NormativeAuthority a owl:Class ;
    rdfs:label "Normative authority" ;
    rdfs:comment "Capacity to confer roles or issue binding directives." ;
    rdfs:subClassOf :Norm .

:NormativeInstitutionalRole a owl:Class ;
    rdfs:label "Normative institutional role" ;
    rdfs:comment "A role defined within an institution." ;
    rdfs:subClassOf :Norm .

:NormativeLegalRole a owl:Class ;
    rdfs:label "Normative legal role" ;
    rdfs:comment "A legal role in a specific legal process or proceeding." ;
    rdfs:subClassOf :Norm .

:BodilyIntegrityRight a owl:Class ;
    rdfs:label "Bodily integrity right" ;
    rdfs:comment "A specific legal right protected in tort law." ;
    rdfs:subClassOf :Right .

:PropertyRight a owl:Class ;
    rdfs:label "Property right" ;
    rdfs:comment "A specific legal right protected in tort law." ;
    rdfs:subClassOf :Right .

:QuietEnjoymentRight a owl:Class ;
    rdfs:label "Quiet enjoyment right" ;
    rdfs:comment "A specific legal right protected in tort law." ;
    rdfs:subClassOf :Right .

:ContractualRight a owl:Class ;
    rdfs:label "Contractual right" ;
    rdfs:comment "A specific legal right protected in tort law." ;
    rdfs:subClassOf :Right .

:ReputationRight a owl:Class ;
    rdfs:label "Reputation right" ;
    rdfs:comment "A specific legal right protected in tort law." ;
    rdfs:subClassOf :Right .

:TortRight a owl:Class ;
    rdfs:label "Tort right" ;
    rdfs:comment "A specific legal right protected in tort law." ;
    rdfs:subClassOf :Right .

:TortDuty a owl:Class ;
    rdfs:label "Tort duty" ;
    rdfs:comment "A duty corresponding to a tort right." ;
    rdfs:subClassOf :Obligation .

:Enforcement a owl:Class ;
    rdfs:label "Enforcement" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:Promise a owl:Class ;
    rdfs:label "Promise" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:Legislation a owl:Class ;
    rdfs:label "Legislation" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:Nullification a owl:Class ;
    rdfs:label "Nullification" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:Adjudication a owl:Class ;
    rdfs:label "Adjudication" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:ContractFormation a owl:Class ;
    rdfs:label "Contract formation" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:Transfer a owl:Class ;
    rdfs:label "Transfer" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:Recognition a owl:Class ;
    rdfs:label "Recognition" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:Violation a owl:Class ;
    rdfs:label "Violation" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:Sanction a owl:Class ;
    rdfs:label "Sanction" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:Repair a owl:Class ;
    rdfs:label "Repair" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:Expiration a owl:Class ;
    rdfs:label "Expiration" ;
    rdfs:comment "A type of legal process or event." ;
    rdfs:subClassOf :LegalEvent .

:AuthoritySource a owl:NamedIndividual , :ChainElement ;
    rdfs:label "Authority source" ;
    :hasDescription "Source of authority to create or enforce norms (e.g., constitution, legislature, court)." .

:NormSpecification a owl:NamedIndividual , :ChainElement ;
    rdfs:label "Norm specification" ;
    :hasDescription "The content of a norm specifying conditions, prohibitions, powers, or permissions." .

:RoleConferral a owl:NamedIndividual , :ChainElement ;
    rdfs:label "Role conferral" ;
    :hasDescription "The conferral of roles or legal capacities upon agents." .

:FactualTrigger a owl:NamedIndividual , :ChainElement ;
    rdfs:label "Factual trigger" ;
    :hasDescription "The factual conditions whose occurrence triggers the application of norms." .

:ProcedureElement a owl:NamedIndividual , :ChainElement ;
    rdfs:label "Procedure element" ;
    :hasDescription "Procedures and processes by which norms are applied or enforced." .

:JurisdictionScope a owl:NamedIndividual , :ChainElement ;
    rdfs:label "Jurisdiction and scope" ;
    :hasDescription "The jurisdictional and target scope of norms and authorities." .

:CorrelativePosition a owl:NamedIndividual , :ChainElement ;
    rdfs:label "Correlative position" ;
    :hasDescription "Positions in correlativity relations, such as rights and corresponding duties." .

:RemedyStage a owl:NamedIndividual , :ChainElement ;
    rdfs:label "Remedy stage" ;
    :hasDescription "The stage at which remedies are determined and applied." .

:TemporalOrder a owl:NamedIndividual , :ChainElement ;
    rdfs:label "Temporal order" ;
    :hasDescription "The temporal sequencing required for legal acts." .

:EffectPosition a owl:NamedIndividual , :ChainElement ;
    rdfs:label "Effect position" ;
    :hasDescription "The legal effect to be achieved by an act." .

:ChainElement a owl:Class ;
    rdfs:label "Chain element" ;
    rdfs:comment "A position in the canonical chain of legality (authority, norm, role, facts, procedure, etc.)." .

:StructuralViolationClass a owl:Class ;
    rdfs:label "Structural violation class" ;
    rdfs:comment "A generic type of mismatch or failure in the legal chain." .

:SourceOfAuthorityFailure a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Source Of Authority Failure" ;
    rdfs:comment "No valid source of authority to create or enforce a norm; authority is not legitimate or recognized." .

:NormativeContradiction a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Normative Contradiction" ;
    rdfs:comment "A norm conflicts with higher-level law or constitutional provisions, making it invalid or defective." .

:ConferralFailure a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Conferral Failure" ;
    rdfs:comment "Authority fails to confer a role properly; the actor lacks legal power to perform the act." .

:RoleContradictionAuthority a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Role Contradiction Authority" ;
    rdfs:comment "An actor does not occupy the appropriate role or acts outside their assigned role." .

:ProceduralContradiction a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Procedural Contradiction" ;
    rdfs:comment "Procedural requirements are not followed; there is a mismatch between the norm and the legal action." .

:TemporalContradiction a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Temporal Contradiction" ;
    rdfs:comment "Timing mismatch between the norm and triggering facts; norms applied retroactively or prematurely." .

:FactualMisalignment a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Factual Misalignment" ;
    rdfs:comment "Triggering facts do not meet normative conditions; the factual predicate for legal action is absent or wrong." .

:JurisdictionalContradiction a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Jurisdictional Contradiction" ;
    rdfs:comment "A norm or authority is applied to a target outside its jurisdiction or scope." .

:RecognitionFailure a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Recognition Failure" ;
    rdfs:comment "The target or legal effect is not recognized by the relevant actors or institutions." .

:CorrelativityContradiction a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Correlativity Contradiction" ;
    rdfs:comment "Rights and duties are not properly correlated; a right is asserted without a corresponding duty." .

:EffectContradiction a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Effect Contradiction" ;
    rdfs:comment "The legal effect contradicts itself or conflicts with existing legal effects." .

:RepairFailure a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Repair Failure" ;
    rdfs:comment "The system fails to repair itself; remedies are not applied or are ineffective." .

:RemedyUnavailableOrBlocked a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Remedy Unavailable Or Blocked" ;
    rdfs:comment "A remedy is legally barred or practically inaccessible, leaving the harm unaddressed." .

:RecognitionRepairFailure a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Recognition Repair Failure" ;
    rdfs:comment "The remedy depends on recognition which is lacking." .

:ProceduralRepairFailure a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Procedural Repair Failure" ;
    rdfs:comment "The procedure needed to obtain a remedy is blocked or incomplete." .

:JurisdictionalRepairFailure a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Jurisdictional Repair Failure" ;
    rdfs:comment "No court has the power to grant the remedy." .

:CompoundContradiction a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Compound Contradiction" ;
    rdfs:comment "Multiple structural contradictions collide in a single case." .

:InstitutionalizedContradiction a owl:Class ;
    rdfs:subClassOf :StructuralViolationClass ;
    rdfs:label "Institutionalized Contradiction" ;
    rdfs:comment "Contradiction that is known to the system but permitted to persist." .

:ChainFailurePattern a owl:Class ;
    rdfs:label "Chain failure pattern" ;
    rdfs:comment "A pattern describing how one or more chain elements are structurally related in a failure." .

:hasOffenseCategory a owl:ObjectProperty ;
    rdfs:label "hasOffenseCategory" ;
    rdfs:comment "Links an offense type to its category." ;
    rdfs:domain :OffenseType ;
    rdfs:range :OffenseCategory .

:hasStructuralViolationClass a owl:ObjectProperty ;
    rdfs:label "hasStructuralViolationClass" ;
    rdfs:comment "Links an offense type to the structural violation classes it instantiates." ;
    rdfs:domain :OffenseType ;
    rdfs:range :StructuralViolationClass .

:hasChainFailurePattern a owl:ObjectProperty ;
    rdfs:label "hasChainFailurePattern" ;
    rdfs:comment "Links an offense type to its chain failure pattern(s)." ;
    rdfs:domain :OffenseType ;
    rdfs:range :ChainFailurePattern .

:focusesOnElement a owl:ObjectProperty ;
    rdfs:label "focusesOnElement" ;
    rdfs:comment "Identifies the primary chain element involved in a failure pattern." ;
    rdfs:domain :ChainFailurePattern ;
    rdfs:range :ChainElement .

:relatesToSecondaryElement a owl:ObjectProperty ;
    rdfs:label "relatesToSecondaryElement" ;
    rdfs:comment "Identifies an additional chain element related to the failure pattern." ;
    rdfs:domain :ChainFailurePattern ;
    rdfs:range :ChainElement .

:involvesFactSituation a owl:ObjectProperty ;
    rdfs:label "involvesFactSituation" ;
    rdfs:comment "Links a legal event to the fact situation it involves." ;
    rdfs:domain :LegalEvent ;
    rdfs:range :FactSituation .

:involvesAgent a owl:ObjectProperty ;
    rdfs:label "involvesAgent" ;
    rdfs:comment "Links a legal event to participating agents." ;
    rdfs:domain :LegalEvent ;
    rdfs:range :Agent .

:appliesNorm a owl:ObjectProperty ;
    rdfs:label "appliesNorm" ;
    rdfs:comment "Links a legal event to norms applied within it." ;
    rdfs:domain :LegalEvent ;
    rdfs:range :Norm .

:confersRole a owl:ObjectProperty ;
    rdfs:label "confersRole" ;
    rdfs:comment "Relates a normative event to the roles it confers." ;
    rdfs:domain :NormativeEvent ;
    rdfs:range :Role .

:hasRemedy a owl:ObjectProperty ;
    rdfs:label "hasRemedy" ;
    rdfs:comment "Links a legal event to the remedies triggered or sought." ;
    rdfs:domain :LegalEvent ;
    rdfs:range :Remedy .

:instantiatesOffenseType a owl:ObjectProperty ;
    rdfs:label "instantiatesOffenseType" ;
    rdfs:comment "Indicates that an offense event instantiates a particular offense type." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :OffenseType .

:qualifies a owl:ObjectProperty ;
    rdfs:label "qualifies" ;
    rdfs:comment "A normative status qualifies a right." ;
    rdfs:domain :NormativeStatus ;
    rdfs:range :Right .

:inheres_in a owl:ObjectProperty ;
    rdfs:label "inheres_in" ;
    rdfs:comment "A normative status inheres in a person or organization." ;
    rdfs:domain :NormativeStatus ;
    rdfs:range :Agent .

:bears a owl:ObjectProperty ;
    rdfs:label "bears" ;
    rdfs:comment "Prohibitions bear normative status." ;
    rdfs:domain :Prohibition ;
    rdfs:range :NormativeStatus .

:bears a owl:ObjectProperty ;
    rdfs:label "bears" ;
    rdfs:comment "Permissions bear normative status." ;
    rdfs:domain :Permission ;
    rdfs:range :NormativeStatus .

:inheres_in a owl:ObjectProperty ;
    rdfs:label "inheres_in" ;
    rdfs:comment "Normative role inheres in a normative status." ;
    rdfs:domain :NormativeRole ;
    rdfs:range :NormativeStatus .

:inheres_in a owl:ObjectProperty ;
    rdfs:label "inheres_in" ;
    rdfs:comment "Normative power inheres in a normative status." ;
    rdfs:domain :NormativePower ;
    rdfs:range :NormativeStatus .

:inheres_in a owl:ObjectProperty ;
    rdfs:label "inheres_in" ;
    rdfs:comment "Normative authority inheres in a normative status." ;
    rdfs:domain :NormativeAuthority ;
    rdfs:range :NormativeStatus .

:grounds a owl:ObjectProperty ;
    rdfs:label "grounds" ;
    rdfs:comment "Authority grounds a role." ;
    rdfs:domain :NormativeAuthority ;
    rdfs:range :NormativeRole .

:degrades a owl:ObjectProperty ;
    rdfs:label "degrades" ;
    rdfs:comment "Power can degrade or limit a role." ;
    rdfs:domain :NormativePower ;
    rdfs:range :NormativeRole .

:specializes a owl:ObjectProperty ;
    rdfs:label "specializes" ;
    rdfs:comment "A normative role specializes a legal role." ;
    rdfs:domain :NormativeRole ;
    rdfs:range :NormativeLegalRole .

:specializes a owl:ObjectProperty ;
    rdfs:label "specializes" ;
    rdfs:comment "An institutional role specializes a legal role." ;
    rdfs:domain :NormativeInstitutionalRole ;
    rdfs:range :NormativeLegalRole .

:realizes a owl:ObjectProperty ;
    rdfs:label "realizes" ;
    rdfs:comment "A legal role realizes an abstract normative role." ;
    rdfs:domain :NormativeLegalRole ;
    rdfs:range :NormativeRole .

:holder a owl:ObjectProperty ;
    rdfs:label "holder" ;
    rdfs:comment "Links a right to the agent who holds it." ;
    rdfs:domain :Right ;
    rdfs:range :Agent .

:bearer a owl:ObjectProperty ;
    rdfs:label "bearer" ;
    rdfs:comment "Links an obligation to the agent who bears the duty." ;
    rdfs:domain :Obligation ;
    rdfs:range :Agent .

:correlatesWith a owl:ObjectProperty ;
    rdfs:label "correlatesWith" ;
    rdfs:comment "Correlates a right with its corresponding duty." ;
    rdfs:domain :Right ;
    rdfs:range :Obligation .

:infringes a owl:ObjectProperty ;
    rdfs:label "infringes" ;
    rdfs:comment "The event infringes a right." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :Right .

:threatens a owl:ObjectProperty ;
    rdfs:label "threatens" ;
    rdfs:comment "The event threatens a right." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :Right .

:usurps a owl:ObjectProperty ;
    rdfs:label "usurps" ;
    rdfs:comment "The event usurps a property right." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :Right .

:violatesBoundaryOf a owl:ObjectProperty ;
    rdfs:label "violatesBoundaryOf" ;
    rdfs:comment "The event violates the boundary of a right." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :Right .

:interferesWith a owl:ObjectProperty ;
    rdfs:label "interferesWith" ;
    rdfs:comment "The event interferes with a right." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :Right .

:typicallyViolates a owl:ObjectProperty ;
    rdfs:label "typicallyViolates" ;
    rdfs:comment "The event typically violates a right." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :Right .

:inducesBreachOf a owl:ObjectProperty ;
    rdfs:label "inducesBreachOf" ;
    rdfs:comment "The event induces breach of a right." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :Right .

:violatesRight a owl:ObjectProperty ;
    rdfs:label "violatesRight" ;
    rdfs:comment "The event violates a right." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :Right .

:injuresReputation a owl:ObjectProperty ;
    rdfs:label "injuresReputation" ;
    rdfs:comment "The event injures reputation associated with a right." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :Right .

:adjudicatedBy a owl:ObjectProperty ;
    rdfs:label "adjudicatedBy" ;
    rdfs:comment "Offense event is adjudicated by a tort judgment." ;
    rdfs:domain :OffenseEvent ;
    rdfs:range :TortJudgment .

:respondsTo a owl:ObjectProperty ;
    rdfs:label "respondsTo" ;
    rdfs:comment "Judgment responds to a tort event." ;
    rdfs:domain :TortJudgment ;
    rdfs:range :OffenseEvent .

:declaresOn a owl:ObjectProperty ;
    rdfs:label "declaresOn" ;
    rdfs:comment "Judgment declares on normative entities." ;
    rdfs:domain :TortJudgment ;
    rdfs:range :Norm .

:records a owl:ObjectProperty ;
    rdfs:label "records" ;
    rdfs:comment "Judgment records into a document." ;
    rdfs:domain :TortJudgment ;
    rdfs:range :TortDocument .

:documentedBy a owl:ObjectProperty ;
    rdfs:label "documentedBy" ;
    rdfs:comment "Document is documented by a judgment." ;
    rdfs:domain :TortDocument ;
    rdfs:range :TortJudgment .

:describes a owl:ObjectProperty ;
    rdfs:label "describes" ;
    rdfs:comment "Document describes a tort event." ;
    rdfs:domain :TortDocument ;
    rdfs:range :OffenseEvent .

:hasName a owl:DatatypeProperty ;
    rdfs:label "has name" ;
    rdfs:range xsd:string .

:hasDescription a owl:DatatypeProperty ;
    rdfs:label "has description" ;
    rdfs:range xsd:string .

:hasRelationType a owl:DatatypeProperty ;
    rdfs:label "has relation type" ;
    rdfs:comment "Encodes relation between chain elements, e.g. FOCUS, FORWARD, BIDIRECTIONAL." ;
    rdfs:domain :ChainFailurePattern ;
    rdfs:range xsd:string .

:OffenseCategory a owl:Class ;
    rdfs:label "Offense category" ;
    rdfs:comment "A category for offenses such as Crime, Tort, or Constitutional violation." .

:Tort a owl:NamedIndividual , :OffenseCategory ;
    rdfs:label "Tort" .

:Constitutional a owl:NamedIndividual , :OffenseCategory ;
    rdfs:label "Constitutional" .

:Crime a owl:NamedIndividual , :OffenseCategory ;
    rdfs:label "Crime" .

:OffenseType a owl:Class ;
    rdfs:label "Offense type" ;
    rdfs:comment "A type-level pattern of wrongful conduct (e.g., Battery, Assault)." .

:TortJudgment a owl:Class ;
    rdfs:label "Tort judgment" ;
    rdfs:comment "A judgment issued in a tort case." .

:DamageJudgment a owl:Class ;
    rdfs:label "Damage judgment" ;
    rdfs:comment "A type of tort judgment." ;
    rdfs:subClassOf :TortJudgment .

